{
  "name": "learn-d3",
  "version": "1.0.0",
  "description": "d3-svg-ssr",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node ./dist/server.js",
    "prod": "npm run clean && webpack",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "prod:custom": "npm run prod -- --env.presets custom",
    "dev-w": "webpack --env.mode development --watch",
    "dev-dev": "webpack-dev-server --env.mode development",
    "dev-start": "nodemon --exec babel-node ./server/server.js",
    "format": "prettier --write \"src/**/*.{js,jsx,html}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --env.mode production",
    "test": "jest"
  },
  "keywords": [
    "react",
    "express.js",
    "react-router",
    "server-side-rendering",
    "isomorphic-react",
    "d3"
  ],
  "author": "RL",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "serialize-javascript": "^3.1.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "d3": "^5.15.1",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
